{
  "editor.tabSize": 2,
  "telemetry.telemetryLevel": "off",
  "gitlens.graph.statusBar.enabled": false,
  "gitlens.menus": {
    "scm": {
      "graph": false
    },
    "scmRepositoryInline": {
      "graph": false
    },
